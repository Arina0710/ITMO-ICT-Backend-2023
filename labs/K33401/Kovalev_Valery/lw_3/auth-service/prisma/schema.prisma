generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id        Int        @id @default(autoincrement())
  email     String     @unique
  password  String
  name      String?
  JWTAccess JWTAccess?
}

model JWTAccess {
  id         String   @id @default(uuid())
  createTime DateTime @default(now())
  user       User     @relation(fields: [userId], references: [id])
  userId     Int      @unique
}
